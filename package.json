{
  "name": "keplr-cro-mvp",
  "version": "0.0.1",
  "description": "Crypto.org Keplr NFT Manager",
  "author": "Crypto.org <chain@crypto.org>",
  "license": "Apache-2.0",
  "homepage": "https://cdc-hitesh.github.io/",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@cosmjs/proto-signing": "^0.25.5",
    "@cosmjs/stargate": "^0.24.0-alpha.22",
    "@crypto-org-chain/chain-jslib": "0.0.22",
    "@keplr-wallet/cosmos": "^0.8.11",
    "@keplr-wallet/types": "^0.8.11",
    "@testing-library/jest-dom": "^5.14.1",
    "@types/big.js": "^6.0.1",
    "@types/crypto-js": "^4.0.1",
    "@types/protobufjs": "^6.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.6",
    "antd": "4.15.6",
    "axios": "^0.21.1",
    "bfj": "^7.0.2",
    "big.js": "^6.0.3",
    "camelcase": "^6.1.0",
    "copyfiles": "^2.4.1",
    "crypto-js": "^4.0.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "less": "3.12.2",
    "less-loader": "7.1.0",
    "less-vars-to-js": "^1.3.0",
    "nedb-promises": "^4.1.5",
    "protobufjs": "^6.11.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-player": "^2.9.0",
    "react-refresh": "^0.8.3",
    "react-router-dom": "^5.2.0",
    "recoil": "^0.1.2",
    "sass-loader": "8.0.2",
    "scrypt-js": "^3.0.1",
    "ts-pnp": "1.2.0",
    "typescript": "4.0.5",
    "url-loader": "4.1.1",
    "zxcvbn": "^4.4.2"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "yarn clean-builds && node scripts/build.js",
    "test": "node scripts/test.js --watchAll=false",
    "clean-builds": "rimraf build && rimraf dist",
    "electron:build": "yarn clean-builds && yarn build && yarn copy-macos-manifest && tsc -p electron && electron-builder --publish=never",
    "lint-staged": "lint-staged",
    "lint:js": "eslint --cache --fix --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "prettier": "prettier --write . '!**/*.{js,jsx,tsx}'"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ],
    "src/**/*.less": [
      "stylelint --fix less",
      "stylelint --fix --syntax less",
      "stylelint --syntax less"
    ],
    "src/**/*.{js,jsx,ts,tsx}": "yarn lint:js"
  },
  "build": {
    "productName": "Crypto.org Chain Desktop Wallet (Beta)",
    "appId": "chain.crypto.org.chain-desktop-wallet",
    "files": [
      "./build/**/*",
      "./dist/**/*",
      "./public/*",
      "./public/**/*"
    ],
    "afterSign": "electron-builder-notarize",
    "mac": {
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "./build/entitlements.mac.plist",
      "entitlementsInherit": "./build/entitlements.mac.plist",
      "target": [
        "dmg"
      ],
      "category": "public.app-category.utilities",
      "darkModeSupport": true
    },
    "linux": {
      "category": "Utility"
    },
    "dmg": {
      "icon": false
    },
    "directories": {
      "buildResources": "build"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@pmmmwh/react-refresh-webpack-plugin": "0.4.2",
    "@types/chai": "4.2.14",
    "@types/chai-as-promised": "^7.1.3",
    "@types/mocha": "8.0.4",
    "@types/moxios": "^0.4.10",
    "@typescript-eslint/eslint-plugin": "4.5.0",
    "@typescript-eslint/parser": "4.5.0",
    "@umijs/fabric": "2.3.1",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.6.0",
    "babel-loader": "8.1.0",
    "babel-plugin-named-asset-import": "0.3.7",
    "babel-preset-react-app": "10.0.0",
    "case-sensitive-paths-webpack-plugin": "2.3.0",
    "chai": "4.2.0",
    "chai-as-promised": "^7.1.1",
    "concurrently": "5.3.0",
    "css-loader": "4.3.0",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "electron": "11.1.1",
    "electron-builder": "22.10.5",
    "eslint": "^7.10.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^3.9.2",
    "eslint-plugin-unicorn": "^23.0.0",
    "eslint-webpack-plugin": "^2.1.0",
    "file-loader": "6.1.1",
    "fs-extra": "9.0.1",
    "html-webpack-plugin": "4.5.0",
    "husky": "4.3.0",
    "jest": "26.6.0",
    "jest-circus": "26.6.3",
    "jest-watch-typeahead": "0.6.1",
    "lint-staged": "10.4.0",
    "mini-css-extract-plugin": "0.11.3",
    "mocha": "8.1.3",
    "moxios": "^0.4.0",
    "optimize-css-assets-webpack-plugin": "5.0.4",
    "pnp-webpack-plugin": "1.6.4",
    "postcss-flexbugs-fixes": "4.2.1",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "8.0.1",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "5.0.2",
    "prettier": "1.19.1",
    "react-app-polyfill": "2.0.0",
    "react-dev-utils": ">=11.0.4",
    "resolve": "1.18.1",
    "resolve-url-loader": "3.1.2",
    "rimraf": "3.0.2",
    "semver": "7.3.2",
    "style-loader": "1.3.0",
    "terser-webpack-plugin": "4.2.3",
    "webpack": "4.44.2",
    "webpack-dev-server": "3.11.0",
    "webpack-manifest-plugin": "2.2.0",
    "workbox-webpack-plugin": "5.1.4"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testRunner": "<rootDir>/node_modules/jest-circus/runner.js",
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": true
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "resolutions": {
    "@umijs/fabric/stylelint-config-rational-order/stylelint": "13.9.0",
    "@umijs/fabric/stylelint-config-rational-order/stylelint/meow/yargs-parser": "20.2.4",
    "immer": "8.0.1",
    "xmldom": "0.5.0",
    "is-svg": "4.2.2",
    "ssri": "8.0.1",
    "hosted-git-info": "3.0.8",
    "underscore": "1.12.1",
    "trim": "0.0.3",
    "dns-packet": "5.2.2",
    "css-what": "5.0.1",
    "trim-newlines": "3.0.1",
    "normalize-url": "5.3.1"
  }
}
